{
  "name": "elghareeb-portfolio",
  "description": "Ahmed Elghareeb - Personal Portfolio",
  "engines": {
    "bun": ">=1.1.0"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.bun": "vite build -c adapters/bun/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "typecheck": "tsc --noEmit",
    "deploy": "echo 'Run \"bun run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "bun --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint:fix": "eslint \"src/**/*.ts*\" --fix",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "clean": "rm -rf dist server tmp .qwik node_modules/.vite",
    "update": "bun outdated"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.13.0",
    "@builder.io/qwik-city": "^1.13.0",
    "@eslint/js": "^9.17.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/bun": "^1.1.14",
    "@types/node": "^22.10.2",
    "@unpic/qwik": "^1.0.0",
    "eslint": "^9.17.0",
    "eslint-plugin-perfectionist": "^4.4.0",
    "eslint-plugin-qwik": "^1.17.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.17",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.18.1",
    "valibot": "^1.2.0",
    "vite": "^5.4.11",
    "vite-plugin-pwa": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
